{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "Commonjs",
        "target": "es5",
        "allowJs": true,
        "moduleResolution": "node",
        "skipLibCheck": true,
        "strict": true,
        "esModuleInterop": true
    }
}
